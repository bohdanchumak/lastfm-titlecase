{
	"manifest_version": 3,
	"name": "Last.fm Title Case",
	"version": "0.1.0",
	"description": "Fixes song, album, and artist name casing on Last.fm",
	"content_scripts": [
		{
			"matches": [
				"https://www.last.fm/*",
				"https://last.fm/*"
			],
			"js": [
				"content.js"
			]
		}
	]
}
