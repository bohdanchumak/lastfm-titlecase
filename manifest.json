{
	"manifest_version": 3,
	"name": "Last.fm Title Case",
	"version": "1.0.0",
	"description": "Fixes song, album, and artist name casing on Last.fm",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.last.fm/*",
				"https://last.fm/*"
			],
			"js": [
				"dist/content.js"
			]
		}
	]
}
